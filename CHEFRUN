#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    float x1,x2,x3,v1,v2,t1,t2,d1,d2;
    cin>>t;
    while(t--)
    {
        cin>>x1>>x2>>x3>>v1>>v2;
        d1=abs(x3-x1);
        d2=abs(x3-x2);
        t1=d1/v1;
        t2=d2/v2;
        if(t1==t2)
        {
            cout<<"Draw"<<"\n";
            
        }
        else if(t1<t2)
        {
            cout<<"Chef"<<"\n";
        }
        else
        {
            cout<<"Kefa"<<"\n";
        }
    }
	// your code goes here
	return 0;
}
