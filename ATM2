#include <stdio.h>

int main(void) {
	long int t,i,j,n,k,arr[100];
	scanf("%ld",&t);
	for(i=1;i<=t;i++)
	{
	    scanf("%ld %ld",&n,&k);
	    for(j=0;j<n;j++)
	    {
	        scanf("%ld",&arr[j]);
	    }
	    for(j=0;j<n;j++)
	    {
	        if(k>=arr[j])
	        {
	            printf("1");
	            k=k-arr[j];
	        }
	        else
	        {
	            printf("0");
	        }
	        
	        
	    }
	    printf("\n");
	    
	}
	return 0;
}
